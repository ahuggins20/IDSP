    "def not_bad(word):\n",
    "    if \"not\" in word:\n",
    "        if \"bad\" in word:\n",
    "            i=word.index(\"not\")\n",
    "            j=word.index(\"bad\")\n",
    "            return word[:i]+\"good\"+word[j+3:]\n",
    "    return word"
   ]
